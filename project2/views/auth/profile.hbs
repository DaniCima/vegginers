<div class="col-12 profile-content">
<button class="btn btn-primary btn-lg"><a class="btnCreatePost" href="/posts/create">Create a new Post</a></button>


<h2 class="titleProfile">Latest Posts</h2>

  {{#each allPosts}}

    <div class="elementsProfile">
<div class="post-container">
  <div class="resize">
        <img src="{{this.imageUrl}}" />
  </div>
  <div class="tiltePostDiv">
      <h3> 
        <a class="titlePosts" href="/posts/single-post/{{this._id}}"> {{this.title}} </a>
      </h3>
      <h4>{{this.history}}</h4>
  </div>
      </div>
</div>
  {{/each}}



</div>
